<%= form_for([:vendors, @deal]) do |f| %>
  <%= error_messages(@deal) %>

  <div class="field">
    <%= f.label :sell_currency %><br>
    <%= f.text_field :sell_currency %>
  </div>
  <div class="field">
    <%= f.label :buy_currency %><br>
    <%= f.text_field :buy_currency %>
  </div>
  <div class="field">
    <%= f.label :rate %><br>
    <%= f.text_field :rate %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
